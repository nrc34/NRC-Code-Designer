<UserControl x:Class="NRC_Code_Designer.src.UI.ToolBox.UserControlToolBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:userControlToolBoxItem="clr-namespace:NRC_Code_Designer.src.UI.ToolBox"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="150">
    <Grid>
        <StackPanel >
            <userControlToolBoxItem:UserControlToolBoxItem 
                Label="Class" 
                ItemImage="{DynamicResource ImageClass}"/>
            <userControlToolBoxItem:UserControlToolBoxItem 
                Label="Interface" 
                ItemImage="{DynamicResource ImageInterface}"/>
        </StackPanel>    
    </Grid>
</UserControl>
